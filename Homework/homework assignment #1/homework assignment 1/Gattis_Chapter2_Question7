#include <iostream>
#include <iomanip>

using namespace std;

/*
 * 
 */
int main(int argc, char** argv) {
    float mpgT= 21.5, mpgH= 26.8, mptT=0 , mptH=0;
    mptT=20*mpgT, mptH= 20*mpgH;
    
// mpgT= miles per gallon in town, mpgH= miles per gallon on the highway, 
// mptT= miles per tank in town, mptH= miles per tank on the highway.    
    cout << "miles per tank in town:" << mptT << endl;
    cout << "miles per tank on the highway" << mptH << endl;
    
    return 0;
}

