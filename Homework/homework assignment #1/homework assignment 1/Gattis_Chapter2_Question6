#include <iostream>

using namespace std;

/*
 * 
 */
int main(int argc, char** argv) {
    
   int mBr= 350, nGg= 16, mpg=0;
    
    mpg= mBr/nGg;
    
// mpg= miles per gallon, mBr= miles before refueling, nGg= number of gallons of gas the car holds.
    cout << "miles per gallon:" << mpg << endl;
    return 0;
}

